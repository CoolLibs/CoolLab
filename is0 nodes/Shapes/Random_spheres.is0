[] float sdBase(vec3 p){
    uvec3 i = uvec3(floor(p));
    vec3 f = fract(p);
    return min(min(min(sph(i,f,uvec3(0,0,0)),
                       sph(i,f,uvec3(0,0,1))),
                   min(sph(i,f,uvec3(0,1,0)),
                       sph(i,f,uvec3(0,1,1)))),
               min(min(sph(i,f,uvec3(1,0,0)),
                      sph(i,f,uvec3(1,0,1))),
                  min(sph(i,f,uvec3(1,1,0)),
                      sph(i,f,uvec3(1,1,1)))));
}