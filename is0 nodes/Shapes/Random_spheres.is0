[] float sdBase(vec3 p){
    vec3 i = floor(p);
    vec3 f = fract(p);
    return min(min(min(sph(i,f,vec3(0,0,0)),
                       sph(i,f,vec3(0,0,1))),
                   min(sph(i,f,vec3(0,1,0)),
                       sph(i,f,vec3(0,1,1)))),
               min(min(sph(i,f,vec3(1,0,0)),
                      sph(i,f,vec3(1,0,1))),
                  min(sph(i,f,vec3(1,1,0)),
                      sph(i,f,vec3(1,1,1)))));
}