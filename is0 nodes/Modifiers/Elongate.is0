[SDF](vec3 p) {
    const vec3 elongation = vec3(X_Elongation, Y_Elongation, Z_Elongation);
    vec3 q = p - clamp(p, -elongation, elongation);
    return ${SDF}(q);
}

float {
    "Name": "X Elongation",
    "Default": 5.0,
    "Min": 0.0,
    "Max": 50.0
}

float {
    "Name": "Y Elongation",
    "Default": 0.0,
    "Min": 0.0,
    "Max": 50.0
}

float {
    "Name": "Z Elongation",
