[SDF] (vec3 pos) {
    const vec3 cell_radii = vec3(X_Spacing, Y_Spacing, Z_Spacing);
    vec3 p = (fract(pos / cell_radii) - 0.5) * cell_radii;
    return ${SDF}(p);
}

float {
    "Name": "X Spacing",
    "Default": 10.0,
    "Min": 0.0,
    "Max": 30.0
}

float {
    "Name": "Y Spacing",
    "Default": 10.0,
    "Min": 0.0,
    "Max": 30.0
}

float {
    "Name": "Z Spacing",
    "Default": 10.0,
    "Min": 0.0,
    "Max": 30.0
}