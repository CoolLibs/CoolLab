[] (vec3 pos) {
    float f = 0.;
    float s = 0.5;
    for (int i = 0; i < Iterations; i++) {
        f += s * iqnoise_3D(pos);
        s *= 0.5;
        pos *= 2.0;
    }
    return IS0_FIX_ARTIFACTS(f);
}

