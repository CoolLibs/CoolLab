(RayMarchRes res)
{
    float fog_amount = 1.0 - exp( -res.distance * Density);
    color = mix(color, Color, fog_amount);
}

float {
    "Name": "Density",
    "Default": 0.01,
    "Min": 0.0,
    "Max": 0.1
}

color {
    "Name": "Color",
    "Default": {
        "x": 1.0,
        "y": 1.0,
        "z": 1.0
    }
}