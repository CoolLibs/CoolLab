INPUT float `Value 1`;
INPUT float `Value 2`;
INPUT float `Random value`;
INPUT float `Probabilty of picking 2`;

float main()
{
    return `Random value` < `Probabilty of picking 2` ? `Value 2` : `Value 1`;
}
